    "next-sitemap": "^4.2.3",
    "node-forge": "^1.3.3",
    "nodemailer": "^7.0.12",
    "passkit-generator": "^3.5.6",
    "payload": "3.73.0",
    "pg": "^8.18.0",
    "prism-react-renderer": "^2.4.1",
    "qrcode.react": "^4.2.0",
    "rate-limiter-flexible": "^9.0.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
