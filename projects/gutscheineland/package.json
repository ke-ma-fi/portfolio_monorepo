    "@types/node": "25.0.3",
    "@types/node-forge": "^1.3.14",
    "@types/nodemailer": "^7.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/sanitize-html": "^2.16.0",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.29.0",
    "eslint-config-next": "16.1.1",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
